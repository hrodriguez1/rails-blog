<h1>New article</h1>

<%= form_with scope: :article, url: articles_path, local: true do |form| %>


  <p>
    <%= form.label :tittle %><br>
    <%= form.text_field :tittle %>
  </p>

  <p>
    <%= form.label :text %><br>
    <%= form.text_area :text %>
  </p>

  <p>
    <%= form.submit %>
  </p>
<% end %>
